{
  "name": "github-browser",
  "version": "1.0.0",
  "description": "A github tool",
  "author": "you06",
  "private": true,
  "scripts": {
    "dev": "cross-env HOST=127.0.0.1 PORT=3000 NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production pm2 start server/index.js --name github-browser",
    "stop": "pm2 stop github-browser",
    "monit": "pm2 monit",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@octokit/rest": "^16.30.1",
    "cross-env": "^5.2.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "mysql": "^2.17.1",
    "nuxt": "^2.0.0",
    "nuxt-buefy": "^0.3.2",
    "pm2": "^4.2.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "nodemon": "^1.18.9"
  }
}
